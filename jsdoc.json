{
  "source": {
    "include": ["./src", "./index.js", "./README.md"]
  },
  "opts": {
    "destination": "/mnt/neuroverse/https-neurons/subDomains/docs/public/netget",
    "template": "/mnt/neuroverse/suign.github.io/Sandbox/better-docs/", 
    "readme": "./README.md"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "search": true,
    "default": {
      "staticFiles": {
        "include": ["./README.md"]
      }
    },
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc"]
    },
    "better-docs": {
      "name": "NetGet",
      "title": "NetGet Documentation",
      "css": "styles/app.min.css",
      "trackingCode": "...",
      "hideGenerator": false,
      "navLinks": [
        {
          "label": "All.This",
          "href": "https://docs.neurons.me/all.this/"
        },
        {
          "label": "Neurons.me",
          "href": "https://neurons.me"
        },
        {
          "label": "Github",
          "href": "https://github.com/neurons-me/all.this"
        }
      ]
        }
  },
  "plugins": [
    "plugins/markdown",
    "/mnt/neuroverse/suign.github.io/Sandbox/better-docs/category.js"
  ]
}
